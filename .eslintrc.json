{
    "env": {
        "node": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "no-console" :1,
        "indent": [
            1,
            "tab"
        ],
        "linebreak-style": [
            1,
            "unix"
        ],
        "quotes": [
            1,
            "double"
        ],
        "semi": [
            1,
            "never"
        ]
    }
}