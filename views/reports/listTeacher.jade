//- Listado de Profesores
//- POST /reports/general
extends ../templates/templatePdf.jade

block content
	section.consul.consulQuery
		h3 Listado General PROFESORES
		section
			button#buttonOpenPDF(type="button").hide
				a#linkpOenPDF(href="#" target="_blank") Mostrar PDF
		article.consulTable
			table
				thead
					tr
						th ID
						th NOMBRES
						th APELLIDOS
						th EDAD
						th TELEFONO
						th CELULAR
						th EMAIL
						th DIRECCION
						th CENTRO
						th USUARIO
						th ESTADO
				tbody
					-var adminusers = dataCustom
					each adminuser in adminusers
						- var teacher = adminuser.idUser ? adminuser.idUser : {}
						tr
							td 
								a(href="/admin/info-user/#{teacher.idUser}" target="blank") #{teacher.idUser}
							td #{teacher.nameUser}
							td #{teacher.lastnameUser}
							td #{teacher.ageUser}
							td #{teacher.telUser}
							td #{teacher.celUser}
							td #{teacher.emailUser}
							td #{teacher.addressUser}
							td #{teacher.centerUser}
							td #{adminuser.userUser}
							td #{parserCustom(adminuser.statusUser,"STATUS_USER")}
